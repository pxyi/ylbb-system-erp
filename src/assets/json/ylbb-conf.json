{
    "name": "活动名称",
    "key": "ylbb-conf",
    "thumbnail": "https://ss1.bdstatic.com/kvoZeXSm1A5BphGlnYG/skin/760.jpg?2",
    "configItems": [
        {
            "label": "活动名称",
            "type": "input",
            "key": "activityName",
            "placeholder": "请输入活动名称",
            "errorText": "请输入2-20位字符!",
            "required": true,
            "pattern": "^.{2,20}$"
        },
        {
            "label": "活动价格",
            "type": "input",
            "key": "activityPrice",
            "placeholder": "请输入活动价格",
            "errorText": "请输入正确的价格!",
            "required": true,
            "pattern": "(^[1-9]([0-9]+)?(\\.[0-9]{1,2})?$)|(^(0){1}$)|(^[0-9]\\.[0-9]([0-9])?$)"
        },
        {
            "label": "活动原价",
            "type": "input",
            "key": "originalPrice",
            "placeholder": "请输入活动原价",
            "errorText": "请输入正确的原价!",
            "required": true,
            "pattern": "(^[1-9]([0-9]+)?(\\.[0-9]{1,2})?$)|(^(0){1}$)|(^[0-9]\\.[0-9]([0-9])?$)"
        },
        {
            "label": "活动日期",
            "type": "rangepicker",
            "key": "rangepicker",
            "valueKey": [
                "startTime",
                "endTime"
            ],
            "placeholder": ["开始日期", "结束日期"],
            "required": true
        }
    ],
    "status": 1,
    "createTime": "2018-03-22",
    "author": "phuhoang"
}